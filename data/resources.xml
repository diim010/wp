        <item>
            <title>What is the density of EPP foam?</title>
            <link>#</link>
            <pubDate>Mon, 03 Feb 2025 12:00:01 +0000</pubDate>
            <dc:creator>admin</dc:creator>
            <guid isPermaLink="false">rf_res_001</guid>
            <description></description>
            <content:encoded><![CDATA[]]></content:encoded>
            <excerpt:encoded><![CDATA[]]></excerpt:encoded>
            <wp:post_id>1001</wp:post_id>
            <wp:post_date>2025-02-03 12:00:01</wp:post_date>
            <wp:post_date_gmt>2025-02-03 12:00:01</wp:post_date_gmt>
            <wp:comment_status>closed</wp:comment_status>
            <wp:ping_status>closed</wp:ping_status>
            <wp:post_name>what-is-the-density-of-epp-foam</wp:post_name>
            <wp:status>publish</wp:status>
            <wp:post_type>rf_resource</wp:post_type>
            <category domain="rf_resource_type" nicename="faq"><![CDATA[FAQ]]></category>
            <category domain="rf_resource_category" nicename="technical"><![CDATA[Technical Specs]]></category>
            <fields>
                <field_resource_mode>faq</field_resource_mode>
                <field_resource_visibility>guest</field_resource_visibility>
                <field_resource_answer><![CDATA[Expanded Polypropylene (EPP) foam density typically ranges from 18g/l to over 100g/l, depending on the specific grade and application requirements. Low-density EPP (18-30g/l) is used for basic cushioning, while high-density EPP (60g/l+) is used for structural automotive components.]]></field_resource_answer>
            </fields>
        </item>

